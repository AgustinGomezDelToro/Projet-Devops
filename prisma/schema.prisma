datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model user {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  password  String
  role      String
  createAt  DateTime @default(now())
  updateAt  DateTime @updatedAt
}


model pacientes {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  password  String
  role      String
  clinicHistory String
  patienDe String
  createAt  DateTime @default(now())
  updateAt  DateTime @updatedAt
}

model odontograma {

}
